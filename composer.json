{
    "name": "timatanga/headless",
    "description": "The timatanga Headless package. Browser automation with chrome/chromium in headless mode",
    "keywords": ["headless", "chromium", "webdriver"],
    "type": "library",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-zip": "^1.19",
        "symfony/finder": "^4.3|^5.0",
        "symfony/process": "^4.3|^5.0",
        "php-webdriver/webdriver": "^1.9.0",
        "symfony/finder": "6.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "10.0.x-dev",
        "symfony/var-dumper": "6.0.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "timatanga\\Headless\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Mark Fluehmann",
            "email": "dbiz.apps@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
